{
  "name": "@awamstock/front",
  "version": "1.0.3",
  "main": "dist/electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "Awam Wang",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:serve": "NODE_ENV=development ts-node -P tsconfig.server.json -r tsconfig-paths/register src/server/index.ts",
    "dev:servew": "NODE_ENV=development nodemon --exec 'ts-node -P tsconfig.server.json -r tsconfig-paths/register' src/server/index.ts",
    "dev:vue": "pnpm run dev:serve & NO_ELECTRON=true pnpm run dev",
    "dev:kraken": "kraken ./dist-kraken/main.js",
    "typecheck:tsc": "tsc -p ./tsconfig.json --noEmit",
    "typecheck": "vue-tsc --noEmit",
    "clean": "rimraf dist/",
    "build:vue": "vite build",
    "build:dts": "vue-tsc --declaration --emitDeclarationOnly",
    "build:ele": "electron-builder -p never",
    "pack": "electron-builder --dir",
    "build:server": "node ./esbuild.server.js",
    "build:kraken": "KRAKEN=true vite build",
    "build:all": "pnpm run clean && pnpm run lint && pnpm run typecheck:tsc && vite build && pnpm run build:server && pnpm run build:ele",
    "build": "pnpm run build:all ",
    "inspect:ele": "electron --inspect=5858 dist/win-unpacked/AwamStock.exe",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue src && pnpm run lint:css",
    "lint:css": "stylelint **/*.vue",
    "update:browserslist": "npx update-browserslist-db@latest",
    "postinstall": "electron-builder install-app-deps"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antv/data-set": "^0.11.8",
    "@antv/g2": "^4.2.10",
    "@awamstock/model": "workspace:^",
    "@awamstock/shared": "workspace:^",
    "@awamwang/vue-socket.io-extended": "0.0.3",
    "@element-plus/icons-vue": "^2.1.0",
    "@iconify-json/ep": "^1.1.10",
    "@iconify-json/wi": "^1.1.4",
    "@rollup/plugin-typescript": "^11.1.0",
    "@sentry/tracing": "^7.47.0",
    "@sentry/vue": "^7.47.0",
    "@types/koa": "^2.13.6",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^16.18.23",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "ag-grid-community": "^28.2.1",
    "autoprefixer": "^10.4.14",
    "axios": "^0.27.2",
    "electron": "^22.3.16",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "element-plus": "^2.3.3",
    "eslint": "^8.38.0",
    "eslint-plugin-vue": "^8.7.1",
    "hqchart": "^1.1.11886",
    "http-proxy-middleware": "^2.0.6",
    "iconv-lite": "^0.6.3",
    "jquery": "^3.6.4",
    "koa": "^2.14.1",
    "koa-router": "^10.1.1",
    "koa2-connect": "^1.0.2",
    "lib-esm": "^0.3.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinia": "2.0.34",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^7.8.3",
    "postcss-sorting": "^7.0.1",
    "rollup": "^3.20.6",
    "sass": "^1.61.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "stylelint": "^15.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "terser": "^5.16.8",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-icons": "^0.14.15",
    "unplugin-vue-components": "^0.24.1",
    "upgrade": "^1.1.0",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.13.14",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-esmodule": "^1.4.4",
    "vite-plugin-inspect": "^0.7.19",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-resolve": "^2.3.0",
    "vite-tsconfig-paths": "^3.6.0",
    "vue": "^3.2.47",
    "vue-class-component": "8.0.0-rc.1",
    "vue-router": "^4.1.6",
    "vue-tsc": "^0.38.9"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "localhost",
    "VITE_DEV_SERVER_PORT": 3000
  },
  "build": {
    "appId": "com.awam.app",
    "productName": "AwamStock",
    "copyright": "Copyright Â© year ${author}",
    "buildDependenciesFromSource": true,
    "files": [
      "dist/**/*",
      "package.json",
      "!**/awam-stock/node_modules/**/*",
      "!**/server/node_modules/**/*",
      "!**/shared/node_modules/*",
      "!**/model/node_modules/*",
      "!**/db/node_modules/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extends": null,
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "runAfterFinish": true
    },
    "publish": {
      "provider": "generic",
      "url": "http://47.108.180.116:8080/packages/awamstock"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ]
}
